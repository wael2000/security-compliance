      <bpmn2:script><![CDATA[java.util.List<ComplianceItem> checkList = new java.util.ArrayList<ComplianceItem>();
ComplianceItem item = new ComplianceItem();
item.setName("Delete redundant rules");
item.setStatus("Not Started");
item.setAutomated(true);
item.setAutomationEngine("Ansible");
item.setAutomationTask("APB001");
checkList.add(item);
item = new ComplianceItem();
item.setName("Delete or disable unused objects");
item.setStatus("Not Started");
item.setAutomated(true);
item.setAutomationEngine("Ansible");
item.setAutomationTask("APB002");
checkList.add(item);
item = new ComplianceItem();
item.setName("Evaluate the order of firewall rules for performance");
item.setStatus("Not Started");
item.setAutomated(false);
checkList.add(item);
item = new ComplianceItem();
item.setName("Remove unused connections");
item.setStatus("Not Started");
item.setAutomated(true);
item.setAutomationEngine("Ansible");
item.setAutomationTask("APB003");
checkList.add(item);
item.setName("Document the rules and changes for future reference");
item.setStatus("Not Started");
item.setAutomated(false);
checkList.add(item);
service.setCheckList(checkList);
kcontext.setVariable("service",service);]]></bpmn2:script>
